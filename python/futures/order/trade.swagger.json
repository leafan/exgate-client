              "$ref": "#/definitions/tradeBalanceResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "exchange",
            "description": "required 交易所名称",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "tags": [
          "FutureTrade"
        ]
      }
